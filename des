	system("python second_run.py /Users/bozelosp/Dropbox/remod/homogeneous_ca3/downloads/files/ $line who_random_apical 5 none shrink percent 25 none none");
}

$t2=time();

$dt=$t2-$t1;

print "Elapsed time: $dt seconds\n";
